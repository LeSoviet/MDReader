{
  "name": "mdreader",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . MDReader --platform=win32 --arch=x64 --out=dist --overwrite --asar --app-version=1.0.0",
    "build-installer": "node build-windows-installer.js"
  },
  "build": {
    "appId": "com.example.mdreader",
    "productName": "MD Reader",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false
    },
    "fileAssociations": [
      {
        "ext": "md",
        "name": "Markdown Document"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "marked": "^16.3.0",
    "monaco-editor": "^0.53.0",
    "prismjs": "^1.30.0"
  },
  "devDependencies": {
    "electron": "^38.2.0",
    "electron-builder": "^25.1.8",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^17.1.2",
    "electron-winstaller": "^5.3.0"
  }
}